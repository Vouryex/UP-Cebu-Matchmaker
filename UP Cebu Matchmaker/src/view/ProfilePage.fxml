            </Tab>
            <Tab text="Music and Movies">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextArea fx:id="musicTxtA" editable="false" layoutX="11.0" layoutY="28.0" prefHeight="184.0" prefWidth="337.0" wrapText="true" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
        </tabs>
      </TabPane>
      <ImageView fitHeight="78.0" fitWidth="235.0" layoutX="365.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true">
