          <Tab text="Gender, Age, and Height">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextArea fx:id="genderTxtA" editable="false" layoutX="11.0" layoutY="28.0" prefHeight="184.0" prefWidth="337.0" wrapText="true" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Body Type">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextArea fx:id="bodyTxtA" editable="false" layoutX="11.0" layoutY="28.0" prefHeight="184.0" prefWidth="337.0" wrapText="true" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
            <Tab text="Sports and Hobbies">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextArea fx:id="sportTxtA" editable="false" layoutX="11.0" layoutY="28.0" prefHeight="184.0" prefWidth="337.0" wrapText="true" />
