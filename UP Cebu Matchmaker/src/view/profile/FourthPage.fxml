<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="280.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.createprofile.FourthPageController">
   <children>
      <ImageView fx:id="profilePicture" fitHeight="200.0" fitWidth="200.0" layoutX="200.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="/resources/default pic.jpg" />
         </image></ImageView>
      <Button fx:id="upload" layoutX="222.0" layoutY="232.0" mnemonicParsing="false" onAction="#uploadPic" text="Upload file from computer" />
   </children>
</AnchorPane>
